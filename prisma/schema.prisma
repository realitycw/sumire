generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./sumire.db"
}

model History {
    history_id       Int     @id @default(autoincrement())
    product_id       String
    product_name     String
    company_name     String?
    transaction_date String
    quantity         Int
}
